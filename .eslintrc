{
    "parser": "babel-eslint",
    "ecmaFeatures": {
        "modules": true,
        "arrowFunctions": true,
        "classes": true,
        "spread":true
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "rules": {
        "no-console": "off",
        "no-trailing-spaces": "off",
        "operator-linebreak": "off"
    },
    "extends": [
        "eslint:recommended", "google"
    ]
}
